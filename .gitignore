# ==============================================
# Wisse Digital Signage - Git Ignore
# ==============================================

# Environment files
.env
.env.local
.env.*.local

# Dependencies
node_modules/
package-lock.json
yarn.lock

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
lerna-debug.log*

# Build outputs
dist/
build/
.next/
out/

# Backend uploads (don't commit user uploads)
backend/uploads/*
!backend/uploads/.gitkeep

# Backend logs
backend/logs/*
!backend/logs/.gitkeep

# Nginx logs
nginx/logs/*
!nginx/logs/.gitkeep

# SSL certificates (don't commit private keys)
nginx/certs/*
!nginx/certs/.gitkeep

# Database data
mysql_data/

# IDE
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store

# OS
Thumbs.db
.DS_Store

# Temporary files
tmp/
temp/
*.tmp

# Docker
.dockerignore

# Testing
coverage/
.nyc_output/

# Misc
*.pem
*.key
*.crt
